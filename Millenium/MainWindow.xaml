<Window x:Name="MenuGrid" x:Class="Millenium.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Millenium Silver" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Width="450" Height="750" Icon="/Millenium_Icon.png">
    <Window.Background>
        <ImageBrush x:Name="BackgroundGame" AlignmentX="Center" AlignmentY="Center" Stretch="Fill" ViewboxUnits="RelativeToBoundingBox" TileMode="None"/>
    </Window.Background>
    <ContentControl x:Name="MainContent">
    </ContentControl>
</Window>
